<!-- MessageTemplateSelector.xaml -->
<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:views="clr-namespace:JsonBuilder.Views.TemplateSelectors"
                    xmlns:models="clr-namespace:JsonBuilder.Core.Models.Messages;assembly=JsonBuilder.Core">
    <DataTemplate x:Key="PickConfirmTemplate">
        <!-- 具体模板内容 -->
    </DataTemplate>

    <DataTemplate x:Key="OrderInsertTemplate">
        <!-- 具体模板内容 -->
    </DataTemplate>

    <views:MessageTemplateSelector x:Key="MessageTemplateSelector"
                                  PickConfirmTemplate="{StaticResource PickConfirmTemplate}"
                                  OrderInsertTemplate="{StaticResource OrderInsertTemplate}"/>
</ResourceDictionary>